{
  "name": "react-native-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-native": "^0.55.17",
    "@types/react-test-renderer": "^16.0.1",
    "concurrently": "^3.5.1",
    "jest-expo": "~27.0.0",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "yarn build && yarn watchAndStart",
    "android": "yarn build && yarn watchAndRunAndroid",
    "ios": "yarn build && yarn watchAndRunIOS",
    "test": "jest",
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf build",
    "build": "yarn clean && yarn tsc",
    "watchAndStart": "concurrently \"yarn baseWatch\" \"yarn baseStart\"",
    "tsc": "tsc",
    "baseStart": "react-native-scripts start",
    "baseWatch": "yarn build -w",
    "baseAndroid": "react-native-scripts android",
    "watchAndRunAndroid": "concurrently \"yarn baseWatch\" \"yarn baseAndroid\"",
    "baseIOS": "react-native-scripts ios",
    "watchAndRunIOS": "concurrently \"yarn baseWatch\" \"yarn baseIOS\"",
    "eject": "react-native-scripts eject"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
